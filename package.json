{
  "name": "brians-chat-app",
  "version": "1.0.0",
  "description": "Brian's cool chat app!",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "^1.2.6",
    "concurrently": "^3.5.0"
  },
  "scripts": {
    "postinstall": "cd chat-app && npm install && cd ../server && npm install",
    "build": "cd chat-app && ng build",
    "start:server": "cd server && npm run start",
    "watch:server": "cd server && npm run watch",
    "watch:client": "cd chat-app && npm run watch",
    "watch": "concurrently \"npm run watch:server\" \"npm run watch:client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianhang/ChatApp.git"
  },
